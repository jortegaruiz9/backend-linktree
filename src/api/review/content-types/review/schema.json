          "localized": true
        }
      },
      "minLength": 20,
      "maxLength": 80,
      "required": true
    },
    "position": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "minLength": 3,
      "maxLength": 30,
      "required": true
    },
    "user": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
