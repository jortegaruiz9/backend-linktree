{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "review": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "minLength": 20,
      "maxLength": 80,
      "required": true
    },
    "position": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "minLength": 3,
      "maxLength": 30,
      "required": true
    },
    "user": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "minLength": 3,
      "required": true
    },
    "logoUrl": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "regex": "https:\\/\\/[a-z0-9]+\\.public\\.blob\\.vercel-storage\\.com\\/[A-Za-z0-9/_\\-]+\\.(jpg|jpeg|png|gif|webp)"
    }
  }
}
